{
  "jupyter.lab.setting-icon-class": "jp-SearchIcon",
  "jupyter.lab.setting-icon-label": "Quick Open",
  "title": "Quick Open",
  "description": "Quick open panel settings",
  "properties": {
    "excludes": {
      "title": "Exclude Patterns",
      "description": "File and directory patterns to exclude from the list",
      "type": "array",
      "items": { "type": "string" },
      "default": ["node_modules", "__pycache__"]
    },
    "relativeSearch": {
      "title": "Relative Search",
      "description": "Whether to search from currently selected directory",
      "type": "boolean",
      "default": false
    },
    "indexingMethod": {
      "title": "Indexing Method",
      "description": "Method to use for file indexing: 'server' uses the backend endpoint, 'frontend' uses the JupyterLab Contents API",
      "type": "string",
      "enum": ["server", "frontend"],
      "default": "server"
    },
    "depth": {
      "title": "Search Depth",
      "description": "Maximum directory depth to search",
      "type": "number",
      "minimum": 1,
      "default": 20
    }
  },
  "jupyter.lab.menus": {
    "main": [
      {
        "id": "jp-mainmenu-view",
        "items": [
          {
            "command": "quickopen:activate",
            "rank": 1
          },
          {
            "type": "separator",
            "rank": 1
          }
        ]
      }
    ]
  },
  "jupyter.lab.shortcuts": [
    {
      "command": "quickopen:activate",
      "keys": ["Accel Shift P"],
      "selector": "body"
    }
  ],
  "additionalProperties": false,
  "type": "object"
}
